<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
        "http://www.w3.org/TR/html4/loose.dtd">
<html lang="zh">
<head>
    <meta charset="UTF-8">
    <title>{{ .Info.Title }}</title>
</head>
<body>
<div>
    {{ range article := .Articles }}
    <div>
        <a href="./{{ article.Category }}/{{ article.AlternativeName }}.html">{{ article.Title }}</a>
        <small>{{ article.UpdatedAt }}</small><br>
        <small>{{ article.FirstSection }}</small>
        <br>
    </div>
    {{ else }}
    <small>No data</small>
    {{ end }}
</div>
</body>
</html>